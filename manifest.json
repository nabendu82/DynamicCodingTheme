{
  "description": "Shows a new theme every hour. The theme is related to coding.",  
  "homepage_url": "https://github.com/nabendu82/DynamicCodingTheme",
  "icons": {
    "48": "icons/logo_48.png",
    "64": "icons/logo_64.png"
  },
  "manifest_version": 2,
  "name": "Coding Dynamic theme",
  "permissions": [
    "alarms",
    "theme"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "version": "2.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55.0a2"
    }
  }
}
